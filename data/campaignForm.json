{
  "campaignCategories": [
    {
      "name": "address",
      "label": "Address",
      "type": "text",
      "required": true,
      "sample": "123 Main St, Cityville"
    },
    {
      "group": "propertyDetails",
      "fields": [
        {
          "name": "bedrooms",
          "label": "Bedrooms",
          "type": "number",
          "required": true,
          "sample": 3
        },
        {
          "name": "bathrooms",
          "label": "Bathrooms",
          "type": "number",
          "required": true,
          "sample": 2.5
        },
        {
          "name": "garage",
          "label": "Garage",
          "type": "number",
          "required": false,
          "sample": 2
        }
      ]
    },
    {
      "group": "landSize",
      "fields": [
        {
          "name": "landSize",
          "label": "Land Size",
          "type": "number",
          "required": true,
          "sample": 5000
        },
        {
          "name": "buildingSize",
          "label": "Building Size",
          "type": "number",
          "required": true,
          "sample": 2500
        }
      ]
    },
    {
      "name": "propertyStatus",
      "label": "Property Status",
      "type": "select",
      "options": ["Archives", "Active", "New"],
      "required": true,
      "sample": "Active"
    },
    {
      "name": "agent",
      "label": "Agent",
      "type": "select",
      "options": ["rhys gebethner"],
      "required": true,
      "sample": "rhys gebethner"
    },
    {
      "name": "images",
      "label": "Images",
      "type": "file",
      "required": true
    },
    {
      "name": "heading",
      "label": "Heading",
      "type": "textarea",
      "required": true,
      "sample": "Beautiful Home with a View"
    },
    {
      "name": "description",
      "label": "Description",
      "type": "textarea",
      "required": true,
      "sample": "This property features stunning views, spacious rooms, and a lovely backyard."
    }
  ]
}
